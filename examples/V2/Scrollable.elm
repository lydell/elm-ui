module Scrollable exposing (main)

import Html.Attributes
import Ui
import Ui.Background

main = Ui.layout [] view

view = Ui.row [ Ui.height Ui.fill ]
    [ Ui.el
        [ Ui.Background.color (Ui.rgb 230 230 230)
        , Ui.height Ui.fill
        , Ui.width (Ui.portion 1)
        ]
        -- I might be misunderstanding how to use `Ui.scrollable`, but without
        -- `contain: size` I get no scrollbar inside the sidebar and instead
        -- get a scrollbar for the entire page.
        (Ui.scrollable
            [ Ui.padding 16
            , Ui.htmlAttribute (Html.Attributes.style "contain" "size")
            ]
            (Ui.text ("Scrollable sidebar. " ++ lorem))
        )
    , Ui.el
        [ Ui.Background.color (Ui.rgb 200 200 200)
        , Ui.height Ui.fill
        , Ui.padding 16
        , Ui.width (Ui.portion 2)
        ]
        (Ui.text "Main area")
    ]

